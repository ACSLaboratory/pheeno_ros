<launch>
  <!-- Load Parameter Server -->
  <rosparam file="$(find pheeno_ros)/config/pheeno_params.yaml" command="load"/>

  <!-- Start Node for Arduino/Teensy and for obstacle avoidance. -->
  <node ns="pheeno" pkg="rosserial_python" type="serial_node.py"
        name="serial_node" args="/dev/ttyACM0"/>
  <node ns="pheeno" pkg="pheeno_ros" type="obstacle_avoidance.py"
        name="pheeno_obstacle_avoidance"/>
  <node ns="pheeno" pkg="pheeno_ros" type="pi_cam_node.py" name="pi_cam"/>

</launch>
